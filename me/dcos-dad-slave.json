{
  "id": "/dcos-dad-slave",
  "instances": 1,
  "container": {
    "type": "DOCKER",
      "volumes": [
          {
              "containerPath": "/sys/fs/cgroup",
              "hostPath": "/sys/fs/cgroup",
              "mode": "RW"
          }
      ],
    "docker": {
      "image": "jeid64/dcos-dad",
      "privileged": true,
      "network": "USER",
      "parameters": [
        {"key": "tmpfs", "value": "/run:rw,exec,nosuid,size=2097152k"},
        {"key": "tmpfs", "value": "/tmp:rw,exec,nosuid,size=2097152k"}
      ],
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "labels": {
            "VIP_0": "/dcos-dad-slave:80"
          },
          "protocol": "tcp",
          "name": "adminrouter-http"
        },
        {
          "containerPort": 443,
          "hostPort": 0,
          "labels": {
            "VIP_1": "/dcos-dad-slave:443"
          },
          "protocol": "tcp",
          "name": "adminrouter-https"
        },
        {
          "containerPort": 2181,
          "hostPort": 0,
          "labels": {
            "VIP_2": "/dcos-dad-slave:2181"
          },
          "protocol": "tcp",
          "name": "zk"
        },
        {
          "containerPort": 5051,
          "hostPort": 0,
          "labels": {
            "VIP_3": "/dcos-dad-slave:5050"
          },
          "protocol": "tcp",
          "name": "mesos-slave"
        },
        {
          "containerPort": 8080,
          "hostPort": 0,
          "labels": {
            "VIP_4": "/dcos-dad-slave:8080"
          },
          "protocol": "tcp",
          "name": "marathon"
        },
        {
          "containerPort": 8181,
          "hostPort": 0,
          "labels": {
            "VIP_5": "/dcos-dad-slave:8181"
          },
          "protocol": "tcp",
          "name": "exhibitor"
        }
    ],
    "forcePullImage": true
    }
  },
  "mem": 2048,
  "cpus": 2,
  "uris": [
          "http://10.0.5.65:8000/dcos-dad-envfile-slave"
  ],
  "ipAddress": {
          "networkName": "dcos"
  }
}
